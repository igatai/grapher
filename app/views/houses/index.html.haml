%h1
  House一覧
%table
  %thead
    %tr
      %th
        ID
      %th
        FirstName
      %th
        Lastname
      %th
        City
      %th
        num_of_people
      %th
        has_child
  %tbody
    - @houses.each do |house|
      %tr
        %td
          = house.id
        %td
          = house.first_name
        %td
          = house.last_name
        %td
          = house.city
        %td
          = house.num_of_people
        %td
          = house.has_child

= form_tag import_houses_path, multipart: true do
  = file_field_tag :file
  = submit_tag "インポート"